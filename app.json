{
  "expo": {
    "name": "udemy-rn-self-posts",
    "slug": "udemy-rn-self-posts",
    "platforms":["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rn-icon.png", 
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.rusak.alexander.dev.rusak-alexander-self-blog-rn",
      "buildNumber":"1.0.0"
    },
    "android":{
      "package":"com.rusak.alexander.dev.rusak_alexander_self_blog_rn",
      "versionCode":1,
      "permissions":["CAMERA","WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
